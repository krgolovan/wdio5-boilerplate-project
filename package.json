{
    "name": "wdio5-boilerplate-project",
    "version": "1.0.0",
    "description": "Here is a boilerplate project for wdio v5",
    "main": "wdio.cong.js",
    "scripts": {
        "pretty": "prettier --write",
        "lint": "jest",
        "precommit": "lint-staged",
